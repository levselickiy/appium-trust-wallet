{
  "compilerOptions": {
    "target": "ES2018",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["node", "@wdio/types", "@wdio/mocha-framework", "@types/chai"],
    "resolveJsonModule": true
  },
  "include": ["tests/**/*", "wdio.conf.ts"],
  "exclude": ["node_modules"]
}
